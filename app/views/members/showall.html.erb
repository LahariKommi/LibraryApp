<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.email %></td>
        <td><%= link_to 'View Booking History', member, action: :history %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
